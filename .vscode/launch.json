    "version": "0.2.0",
    "configurations": [

  
        {
            "name": "Python: Flask",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "service/runserver.py",
                "FLASK_ENV": "development",
                "FLASK_RUN_PORT": "5001",
                "FLASK_RUN_HOST": "0.0.0.0"
            },
            "args": [
                "run",
                "--no-debugger"
            ],
            "jinja": true,
            "justMyCode": false
        },
        {
            "name": "Python: Django",
            "type": "python",
            "request": "launch",
            "program": "admin/manage.py",
            "args": [
                "runserver",
            ],
            "django": true
        }
    ]
}